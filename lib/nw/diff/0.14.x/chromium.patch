+  assert(content::g_force_cpu_draw);
+
+  AcceleratedWidgetMac* accelerated_widget_mac =
+  GetHelperFromAcceleratedWidget(widget);
+  if (accelerated_widget_mac)
+    accelerated_widget_mac->GotSoftwareFrame(scale_factor, canvas);
+}
