{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["DOM", "DOM.Iterable", "ES2022"],
        "jsx":"preserve",
        "jsxFactory": "VSX.createElement",
        "jsxFragmentFactory": "VSX.createFragment",
        "moduleResolution": "node",
        "isolatedModules": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "declaration": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
